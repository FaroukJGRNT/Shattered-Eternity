script/source = "extends InteractibleLevelObject

var spell_to_give : Item

func _ready() -> void:
	# Randomly take a spell
	# First choose what element
	var elem = Toolbox.roll_element()
	# Then choose which spell
	if elem == \"fire\":
		$AnimatedSprite2D.modulate = Color.hex(0xff6a00ff)
		spell_to_give = Toolbox.fire_spells[randi_range(0, len(Toolbox.fire_spells) - 1)]
	elif elem == \"ice\":
		$AnimatedSprite2D.modulate = Color.hex(0x00eaffff)
		spell_to_give = Toolbox.ice_spells[randi_range(0, len(Toolbox.ice_spells) - 1)]
	elif elem == \"thunder\":
		$AnimatedSprite2D.modulate = Color.hex(0xf2ff00ff)
		spell_to_give = Toolbox.thunder_spells[randi_range(0, len(Toolbox.thunder_spells) - 1)]

var info_modal : PackedScene = load(\"res://ui/molecules/new_item_modal.tscn\")

func interact():
	print(\"Spell giver interaction\")
	if not interacted:
		interacted = true
		player_ref.give_item(spell_to_give)
		print(\"Gave spell \", spell_to_give.item_name, \" to player\")
		var modal = ui_manager.show_modal(info_modal)
		modal.item = spell_to_give
		modal.setup_labels()
		$AnimatedSprite2D.play(\"destruction\")

func _on_animated_sprite_2d_animation_finished() -> void:
	if $AnimatedSprite2D.animation == \"destruction\":
		queue_free()
"

